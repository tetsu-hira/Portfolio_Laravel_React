{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "lint": "next lint --dir src",
    "lint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "format": "prettier --write --ignore-path .gitignore './**/**/*.{js,jsx,ts,tsx,json,css,php}'"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.13.13",
    "@popperjs/core": "^2.10.2",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/redux": "^3.6.0",
    "axios": "^0.25",
    "bootstrap": "^5.1.3",
    "eslint-config-prettier": "^8.5.0",
    "laravel-mix": "^6.0.6",
    "lodash": "^4.17.19",
    "postcss": "^8.1.14",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "redux": "^4.2.0"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100
  }
}
